<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="ContactExtract_Prod"
          class="com.salesforce.dataloader.process.ProcessRunner"
          singleton="false">
      <description>ContactExtract job gets contact info from salesforce and saves info into a CSV file.</description>
        <property name="name" value="ContactExtract_Prod"/>
        <property name="configOverrideMap">
            <map>
                <entry key="sfdc.debugMessages" value="false"/>
                <entry key="sfdc.debugMessagesFile" value="c:\dataloader\samples\status\sfdcSoapTrace.log"/>
                <entry key="sfdc.endpoint" value="https://imagineinstitute.my.salesforce.com"/> 
                <entry key="sfdc.username" value="db.ii@501commons.org"/>
                <entry key="sfdc.password" value="91734f36223b85ee36e9dcdba328b79765b921d98d4aa2fb12c062da0875c458cd9ea26f3014b26b4d38599ef17c98b0ab8a7e532f4a712ffa52d3773589796e"/>
                <entry key="process.encryptionKeyFile" value="C:\repo\Salesforce-Exporter-Private\Clients\II\DataLoader\key.txt"/>
                <entry key="sfdc.timeoutSecs" value="600"/>
                <entry key="sfdc.loadBatchSize" value="200"/>
                <entry key="sfdc.entity" value="Contact"/>
                <entry key="sfdc.extractionRequestSize" value="500"/>
                <entry key="sfdc.extractionSOQL" value="Select Id, FirstName, LastName, ii_Primary_Contact__c, ii_Substitute_Pool_Status__c, C501_Cpr_Cert_Days_Remaining__c, C501_First_Aid_Cert_Days_Remaining__c, C501_Food_Handler_Cert_Remaining_Days__c, C501_Safe_Sleep_Cert_Remaining_Days__c, C501_PBC_Cert_Remaining_Days__c, PREPARE_Cohort__c, C501_Certs_Expiring_Message__c, PREPARE_Class__c, RecordTypeId FROM Contact"/>
                <entry key="process.operation" value="extract"/>
                <entry key="process.mappingFile" value="C:\repo\Salesforce-Exporter-Private\Clients\II\Salesforce-Exporter\Clients\II\DataLoader\ContactExtract.sdl"/>
                <entry key="dataAccess.type" value="csvWrite"/>
                <entry key="dataAccess.name" value="C:\repo\Salesforce-Exporter-Private\Clients\II\Salesforce-Exporter\Clients\II\Export\ContactExtract-Prod.csv"/>
            </map>
        </property>
    </bean>
</beans>
